<?xml version="1.0"?>
<launch>
    <group ns="leader">
   		<node name="my_moveit_node" pkg="franka_teleoperation" type="data_collection.py" 
        respawn="false" 	output="screen" />    
	</group> 


    <arg name="robot_ip" default="172.16.0.2"/>
    <arg name="load_gripper" default="False"/>
    <arg name="noserver" default="0"/>

    <!-- CONTROLLER 
    <include file="$(find franka_control)/launch/franka_control.launch">
        <arg name="robot_ip" value="$(arg robot_ip)"/>
        <arg name="load_gripper" value="$(arg load_gripper)" />
    </include> -->

    <!-- MOVE IT -->
    <include file="$(find panda_moveit_config)/launch/panda_moveit.launch" />

    <!-- RVIZ -->
    <include file="$(find panda_moveit_config)/launch/moveit_rviz.launch" />


	<node name="realsense_camera_side" pkg="franka_teleoperation" type="realsense_camera.py" output="screen"/>
	<node name="realsense_camera_front" pkg="franka_teleoperation" type="realsense_camera_front.py" output="screen"/>
    <!-- <node name="finger_camera" pkg="franka_teleoperation" type="server.py" output="screen"/> -->


</launch>